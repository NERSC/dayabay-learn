{
    "language"        : "PYTHON",
    "experiment-name" : "dayabay-conv-ae",
    "polling-time"    : 1,
    "resources" : {
        "my-machine" : {
            "scheduler"         : "local",
            "max-concurrent"    : 1,
            "max-finished-jobs" : 100
        },
        "cluster" : {
            "scheduler"         : "SLURM",
            "max-concurrent"    : 3,
            "max-finished-jobs" : 1000
        }
    },
    "tasks": {
        "branin" : {
            "type"       : "OBJECTIVE",
            "likelihood" : "NOISELESS",
            "main-file"  : "conv_ae_dayabay",
            "resources"  : ["my-machine", "cluster"]
        }
    },
    "variables": {
        "bneck_width" : {
            "type" : "INTEGER",
            "size" : 1,
            "min"  : 10,
            "max"  : 100
        },
        "learning_rate" : {
            "type" : "FLOAT",
            "size" : 1,
            "min"  : 0.000001,
            "max"  : 0.01
        }
        "number_of_filters"
    }
}